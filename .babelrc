{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"plugins": [
		["@babel/plugin-transform-runtime", { "regenerator": true }],
		[
			"@dr.pogodin/react-css-modules",
			{
				"generateScopedName": "[name]_[local]_[hash:base64:5]",
				"filetypes": {
					".scss": {
						"syntax": "postcss-scss"
					}
				}
			}
		]
	]
}
